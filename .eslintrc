{
  "root": true,
  "extends": [
    "@xylabs"
  ],
  "ignorePatterns": ["node_modules", "build", "coverage", "dist", "bin", "storybook-static", ".yarn"],
  "rules": {
    "import/no-nodejs-modules": ["off"],
    "import/no-default-export": "warn",
    "import/no-internal-modules": ["warn", {"allow": [
      "lodash/*"
    ]}]
  }
}