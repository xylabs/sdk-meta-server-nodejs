{
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/xylabs/sdk-meta-server-nodejs/issues"
  },
  "bin": {
    "docker-build": "dist/cjs/bin/docker-build.js",
    "generate-dotenv": "dist/cjs/bin/generate-dotenv.js",
    "start-meta": "dist/cjs/bin/start-meta.js"
  },
  "dependencies": {
    "@xylabs/assert": "^2.6.11",
    "@xylabs/sdk-api-express-ecs": "^1.2.5",
    "@xyo-network/api": "^2.42.10",
    "@xyo-network/core": "^2.42.10",
    "@xyo-network/sdk-meta": "^2.7.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.1",
    "mime": "^3.0.0",
    "pako": "^2.1.0",
    "serve-static": "^1.15.0",
    "typedoc": "^0.23.23",
    "yargs": "^17.6.2"
  },
  "description": "Meta Server - adjusts meta data on index.html for served static site",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@types/serve-static": "^1.15.0",
    "@types/supertest": "^2.0.12",
    "@xylabs/eslint-config": "^2.9.0",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts": "^2.9.0",
    "@xylabs/tsconfig": "^2.9.0",
    "axios": "^1.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xylabs.com",
  "keywords": [
    "xylabs",
    "nodejs",
    "static",
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0",
  "name": "@xylabs/meta-server",
  "peerDependencies": {
    "axios": ">0.26",
    "typedoc": "^0.22.13"
  },
  "peerDependenciesMeta": {
    "typedoc": {
      "optional": true
    }
  },
  "resolutions": {
    "bn.js": "^5.2.0",
    "webpack": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/sdk-meta-server-nodejs.git"
  },
  "scripts": {
    "coverage": "yarn jest --coverage",
    "lint-pkg": "npmPkgJsonLint .",
    "start": "node dist/cjs/bin/start-meta.js",
    "build": "yarn clean && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json && yarn gen:docs",
    "gen:docs": "typedoc ./src/index.ts --json ./dist/docs.json --tsconfig ./tsconfig.json"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "1.0.15",
  "packageManager": "yarn@3.1.1"
}
