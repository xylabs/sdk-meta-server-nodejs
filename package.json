    "he": "^1.2.0",
    "http-status-codes": "^2.3.0",
    "lru-cache": "^11.1.0",
    "mime": "^4.0.7",
    "minimatch": "^10.0.3",
    "puppeteer": "^24.15.0",
    "serve-static": "^2.2.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "date-fns": "^4.1.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/express": "^5.0.3",
    "@types/he": "^1.2.3",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^24.1.0",
    "@types/serve-static": "^1.15.8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/snapshot": "^3.2.4",
    "@xylabs/eslint-config-flat": "^7.0.2",
    "@xylabs/ts-scripts-yarn3": "^7.0.2",
    "@xylabs/tsconfig": "^7.0.2",
    "@xylabs/vitest-extended": "^4.15.3",
    "@xylabs/vitest-matchers": "^4.15.3",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "jest-image-snapshot": "^6.5.1",
    "knip": "^5.62.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "axios": "^1"
  },
  "packageManager": "yarn@4.9.0",
  "engines": {
    "node": ">=22"
  },
